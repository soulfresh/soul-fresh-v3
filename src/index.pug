extends views/layout

block content
  - projects.sort((a, b) => b.year - a.year)
  .header(name="header")
    h1
      span.container.soul-container
        span.soul= 'Soul'
      span.container.fresh-container
        span.fresh= 'Fresh'
  .content(name="root")
    #work.work
      each project in projects
        include views/project.pug

